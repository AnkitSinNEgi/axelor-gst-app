<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.4.xsd">
  
  <module package="com.axelor.gst.app" name="gst"/>
  
  <entity name="Sequence" cacheable="true">
  	<string name="prefix"/>
  	<string name="suffix"/>
  	<integer name="padding" min="1" max="10"/>
  	<string name="nextNumber"/>
  	<many-to-one ref="com.axelor.meta.db.MetaModel" name="model" required="true"/>
  </entity>
  
 </domain-models>